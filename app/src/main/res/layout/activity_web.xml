<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_web"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="chongci.myapplication.activity.WebActivity">

    <ImageView
        android:id="@+id/back_iv"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/fenlei_fanhui"/>

    <LinearLayout
        android:id="@+id/xiamian"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:gravity="center_horizontal">
        <ImageView
            android:id="@+id/shoucang_iv"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/personal_shoucang_img"/>
        <ImageView
            android:id="@+id/fenxiang_iv"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:src="@drawable/pe_share_img" />
    </LinearLayout>

    <WebView
        android:id="@+id/webView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/xiamian"
        android:layout_below="@id/back_iv"></WebView>
</RelativeLayout>
